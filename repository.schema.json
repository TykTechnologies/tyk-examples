{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "examples": {
            "type": "object",
            "required": [
                "udg"
            ],
            "properties": {
                "udg": {
                    "type": "array",
                    "default": [],
                    "items": {
                        "type" : "object",
                        "required": [
                            "location",
                            "name",
                            "description",
                            "features",
                            "minTykVersion"
                        ],
                        "properties": {
                            "location": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            },
                            "features": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "minTykVersion": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    }
}


