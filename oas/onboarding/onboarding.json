    "info": {
      "description": "API for managing coffee shop orders",
      "title": "Coffee Shop API",
      "version": "v1"
    },
    "openapi": "3.0.3",
    "paths": {
      "/menu": {
        "get": {
          "description": "Retrieves the complete menu of available coffee and food items.",
          "operationId": "menuGET",
          "responses": {
            "200": {
              "content": {
                "application/json": {
                  "schema": {
                    "example": [
                      {
                        "description": "A shot of rich, dark coffee",
                        "id": 1,
                        "name": "Tykspresso",
                        "price": 2.5
                      },
                      {
                        "description": "Espresso with steamed milk and foam",
                        "id": 2,
                        "name": "CappuTyko",
                        "price": 3.5
                      }
                    ],
                    "items": {
                      "properties": {
                        "description": {
                          "type": "string"
                        },
                        "id": {
                          "format": "int64",
                          "type": "integer"
                        },
                        "name": {
                          "type": "string"
                        },
                        "price": {
                          "format": "float",
                          "type": "number"
                        }
                      },
                      "type": "object"
                    },
                    "type": "array"
                  }
                }
              },
              "description": "Successful response"
            }
          },
          "summary": "Get the full menu"
        }
      },
      "/orders": {
        "post": {
          "description": "Creates a new order.",
          "operationId": "ordersPOST",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "address": "123 Coffee St, Barista City",
                    "contactNumber": "123456789",
                    "customerName": "John Doe",
                    "items": [
                      {
                        "itemId": 1,
                        "quantity": 2
                      },
                      {
                        "itemId": 3,
                        "quantity": 1,
                        "specialInstructions": "No sugar"
                      }
                    ]
                  },
                  "properties": {
                    "address": {
                      "type": "string"
                    },
                    "contactNumber": {
                      "type": "string"
                    },
                    "customerName": {
                      "type": "string"
                    },
                    "items": {
                      "items": {
                        "properties": {
                          "itemId": {
                            "format": "int64",
                            "type": "integer"
                          },
                          "quantity": {
                            "type": "integer"
                          },
                          "specialInstructions": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "itemId",
                          "quantity"
                        ],
                        "type": "object"
                      },
                      "type": "array"
                    }
                  },
                  "required": [
                    "items"
                  ],
                  "type": "object"
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "content": {
                "application/json": {
                  "schema": {
                    "example": {
                      "orderId": 123
                    },
                    "properties": {
                      "orderId": {
                        "format": "int64",
                        "type": "integer"
                      }
                    },
                    "type": "object"
                  }
                }
              },
              "description": "Order created successfully"
            }
          },
          "summary": "Place an order"
        }
      },
      "/orders/{orderId}": {
        "get": {
          "description": "Retrieves details of a specific order.",
          "operationId": "orders/{orderId}GET",
          "parameters": [
            {
              "description": "ID of the order",
              "in": "path",
              "name": "orderId",
              "required": true,
              "schema": {
                "format": "int64",
                "type": "integer"
              }
            }
          ],
          "responses": {
            "200": {
              "content": {
                "application/json": {
                  "schema": {
                    "example": {
                      "address": "123 Coffee St, Barista City",
                      "contactNumber": "123456789",
                      "customerName": "John Doe",
                      "items": [
                        {
                          "itemId": 1,
                          "name": "Tykspresso",
                          "quantity": 2
                        },
                        {
                          "itemId": 3,
                          "name": "Latte",
                          "quantity": 1,
                          "specialInstructions": "No sugar"
                        }
                      ],
                      "orderId": 123,
                      "status": "Preparing",
                      "totalPrice": 8.5
                    },
                    "properties": {
                      "address": {
                        "type": "string"
                      },
                      "contactNumber": {
                        "type": "string"
                      },
                      "customerName": {
                        "type": "string"
                      },
                      "items": {
                        "items": {
                          "properties": {
                            "itemId": {
                              "format": "int64",
                              "type": "integer"
                            },
                            "name": {
                              "type": "string"
                            },
                            "quantity": {
                              "type": "integer"
                            },
                            "specialInstructions": {
                              "type": "string"
                            }
                          },
                          "type": "object"
                        },
                        "type": "array"
                      },
                      "orderId": {
                        "format": "int64",
                        "type": "integer"
                      },
                      "status": {
                        "type": "string"
                      },
                      "totalPrice": {
                        "format": "float",
                        "type": "number"
                      }
                    },
                    "type": "object"
                  }
                }
              },
              "description": "Order details retrieved successfully"
            },
            "404": {
              "description": "Order not found"
            }
          },
          "summary": "Get order details"
        }
      }
    },
